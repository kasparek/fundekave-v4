RewriteEngine on

RewriteCond %{HTTP_HOST} ^www.fundekave.net$
RewriteRule (.*) http://fundekave.net/$1 [R=301,QSA,L]

RewriteBase /test/
RewriteRule (.*\.css)$ ca.php?c=$1 [L]
RewriteRule (.*\.js)$ ca.php?c=$1 [L]
RewriteRule (.*\.gif)$ ca.php?c=$1 [L]
RewriteRule (.*\.jpg)$ ca.php?c=$1 [L]
RewriteRule (.*\.png)$ ca.php?c=$1 [L]
RewriteRule (.*\.ico)$ ca.php?c=$1 [L]
